<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="TestApp">

  <!-- Inherit the core Web Toolkit stuff.                  -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.ait.lienzo.Lienzo' />

  <!-- Specify the app entry point class.                   -->
  <entry-point class='com.testApp.client.TestApp'/>


  <!-- Specify the app servlets.                   -->
  <servlet path='/TestAppService' class='com.testApp.server.TestAppServiceImpl'/>

</module>